{
  "name": "epub-to-markdow",
  "version": "1.0.0",
  "description": "reading book with tiddlywiki",
  "main": "index.js",
  "scripts": {
    "start": "tiddlywiki --listen",
    "build:books": "ts-node ./src/index.ts",
    "build:ibooks": "ts-node ./src/index.ts -i",
    "format": "prettier  --write src/**",
    "lint": "eslint src",
    "build": "ts-node ./src/index.ts && tiddlywiki --build"
  },
  "keywords": [
    "books",
    "markdown"
  ],
  "author": "oeyoews",
  "license": "MIT",
  "dependencies": {
    "@types/markdown-it": "^13.0.5",
    "@types/prompts": "^2.4.7",
    "chalk": "^4.1.2",
    "cheerio": "1.0.0-rc.12",
    "highlight.js": "^11.9.0",
    "markdown-it": "^13.0.2",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-smartarrows": "^1.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-checkbox": "^1.0.6",
    "markdown-it-task-lists": "^2.1.1",
    "prettier": "^3.0.3",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.5",
    "tiddlywiki": "github:Jermolene/TiddlyWiki5",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "tsconfig-paths": "^4.2.0"
  }
}
